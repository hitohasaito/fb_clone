<%= form_with(model: @facebook, local: true) do |form| %>
  <div class="fb_name">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>
  <div class="fb_name">
    <%= form.label :content %>
    <%= form.text_field :content %>
  </div>
  <div class="fb_name">
    <%= form.label :image %>
    <%= form.file_field :image %>
  </div>
  <%= form.submit %>
<% end %>
